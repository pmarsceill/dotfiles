	smudge = git-lfs smudge %f
	required = true
[user]
	name = Patrick Marsceill
	email = patrick.marsceill@gmail.com
	signingkey = AEAF30C14952BCC5

[hub]
protocol = https

[core]
	excludesfile = /Users/pmarsceill/.gitignore_global
